{
  "name": "rm-up",
  "version": "1.0.0",
  "description": "Delete files or empty directories and their empty parents from bottom to up.",
  "author": {
    "name": "Özüm Eldoğan",
    "email": "ozum@ozum.net",
    "url": ""
  },
  "keywords": [
    "rm",
    "rmdir",
    "del",
    "delete",
    "dir",
    "directory",
    "folder",
    "empty",
    "recurse",
    "recursive",
    "remove",
    "bottom",
    "up",
    "top"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/@(*.spec|*.test)*",
    "!dist/**/__test__",
    "module-files",
    "@types"
  ],
  "homepage": "https://github.com/ozum/rm-up",
  "bugs": "https://github.com/ozum/rm-up/issues",
  "repository": "ozum/rm-up",
  "license": "MIT",
  "scripts": {
    "execute": "ts-node --transpile-only --compiler-options '{ \"module\": \"commonjs\" }'",
    "watch": "ts-node-dev --no-notify --respawn --transpile-only --compiler-options '{ \"module\": \"commonjs\" }'",
    "lint": "eslint --ignore-path .gitignore --cache --max-warnings 0 --ext js,jsx,ts,tsx,vue --fix .",
    "format": "prettier . --write",
    "release": "npm run readme && git pull && git add -A && git commit && git push --follow-tags",
    "pretest": "not-sync coverage",
    "test": "jest --coverage",
    "readme": "node module-files/scripts/tsmod.js readme",
    "prebuild": "not-sync dist",
    "build": "tsc",
    "preinstall": "npx not-sync node_modules",
    "postinstall": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "tsmod": "node module-files/scripts/tsmod.js",
    "yo:update": "yo tsmod:uninstall --no-install --force && yo 'tsmod' '--typedoc'"
  },
  "dependencies": {
    "junk": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ozum/pinst": "^2.1.4",
    "@types/fs-extra": "^9.0.6",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "commitizen": "^4.2.3",
    "concat-md": "^0.3.5",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "fs-extra": "^9.1.0",
    "husky": "^5.0.9",
    "is-ci": "^2.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "not-sync": "^1.4.0",
    "prettier": "^2.2.1",
    "readmeasy": "^0.2.7",
    "tmp-promise": "^3.0.2",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "typedoc": "^0.20.23",
    "typedoc-neo-theme": "^1.1.0",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.4",
    "walkdir": "^0.4.1"
  }
}
